{
  "manifest_version": 2,
  "name": "Netflix Shut Up",
  "description": "Prevent Netflix from deactivating console",
  "version": "0.1",

  "background": {
      "scripts": ["background.js"]
  },

  "icons": {
  },

  "web_accessible_resources": [
  ],

  "permissions": [
      "tabs"
  ],
 
  "content_security_policy": "script-src 'self'; object-src 'self'"
}
